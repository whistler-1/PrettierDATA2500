{
  "manifest_version": 3,
  "name": "Prettier DATA2500",
  "description": "Adds custom CSS",
  "version": "1.0",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "64": "logo64.png",
    "128": "logo128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.oslomet.no/*"],
      "css": ["newStyle.css"]
    }
  ]
}